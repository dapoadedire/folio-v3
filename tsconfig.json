{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*.astro"
      ],
      "@/layouts/*": [
        "src/layouts/*.astro"
      ],
      "@/utils": [
        "src/utils/index.ts"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/data/*": [
        "src/data/*"
      ],
      "@/site-config": [
        "src/site.config.ts"
      ]
    },
  }
}